package com.te.lms.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import com.te.lms.entity.EducationDetails;

@Repository
public interface EducationDetailsRepository extends JpaRepository<EducationDetails, Integer> {

	@Query(value = "select * from employee_education_info where employee_fk=?1", nativeQuery = true)
	List<EducationDetails> findEducationDetails(String empId);

}
