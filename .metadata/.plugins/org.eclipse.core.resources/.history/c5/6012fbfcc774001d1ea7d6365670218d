package com.te.lms.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import com.te.lms.entity.AddressDetails;

@Repository
public interface AddressDetailsRepository extends JpaRepository<AddressDetails, Integer> {

	@Query(value = "select * from employee_address_info where employee_fk=?1", nativeQuery = true)
	List<AddressDetails> findAddressDetails(String empId);

}
